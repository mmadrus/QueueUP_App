<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="pane" prefHeight="700.0" prefWidth="1200.0"
            xmlns="http://javafx.com/javafx/9" fx:controller="Client.RegController">
    <children>
        <TextField fx:id="usernameField" alignment="CENTER" layoutX="447.0" layoutY="60.0" prefHeight="51.0"
                   prefWidth="313.0" promptText="Enter username"/>
        <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="447.0" layoutY="140.0" prefHeight="51.0"
                       prefWidth="313.0" promptText="Enter password"/>
        <PasswordField fx:id="confirmPasswordField" alignment="CENTER" layoutX="447.0" layoutY="220.0" prefHeight="51.0"
                       prefWidth="313.0" promptText="Confirm password"/>
        <TextField fx:id="emailField" alignment="CENTER" layoutX="447.0" layoutY="300.0" prefHeight="51.0"
                   prefWidth="313.0" promptText="Enter email"/>
        <TextField fx:id="confirmEmailField" alignment="CENTER" layoutX="447.0" layoutY="380.0" prefHeight="51.0"
                   prefWidth="313.0" promptText="Confirm email"/>
        <Button fx:id="cancelButton" layoutX="447.0" layoutY="463.0" mnemonicParsing="false" onAction="#cancel"
                prefHeight="31.0" prefWidth="100.0" text="Cancel"/>
        <Button fx:id="registerButton" layoutX="660.0" layoutY="463.0" mnemonicParsing="false"
                onAction="#registerComplete" prefHeight="31.0" prefWidth="100.0" text="Register"/>
    </children>
</AnchorPane>
