<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerThree">
   <children>
      <TabPane fx:id="tabPane" layoutX="200.0" prefHeight="600.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="#General">
            <content>
              <AnchorPane layoutX="50.0" layoutY="50.0" minHeight="0.0" minWidth="0.0" prefHeight="600.0" prefWidth="800.0">
                     <children>
                        <TextArea fx:id="messageArea" editable="false" focusTraversable="false" layoutY="1.0" prefHeight="563.0" prefWidth="800.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="#Help">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <Button layoutX="200.0" layoutY="627.0" mnemonicParsing="false" onAction="#sendMessage" prefHeight="60.0" prefWidth="800.0" text="Send">
         <graphic>
            <TextField fx:id="messageField" layoutX="150.0" layoutY="150.0" prefHeight="50.0" prefWidth="744.0" />
         </graphic>
      </Button>
      <TextArea editable="false" focusTraversable="false" layoutX="5.0" layoutY="34.0" prefHeight="564.0" prefWidth="190.0" />
      <TextArea fx:id="channelUser" editable="false" focusTraversable="false" layoutX="1005.0" layoutY="33.0" prefHeight="564.0" prefWidth="190.0" />
      <Label layoutX="70.0" layoutY="6.0" text="Channels" />
      <Label layoutX="1074.0" layoutY="6.0" text="Users" />
      <Button fx:id="logoutButton" layoutX="1036.0" layoutY="627.0" mnemonicParsing="false" onAction="#logout" prefHeight="60.0" prefWidth="120.0" text="Logout" />
   </children>
</AnchorPane>
